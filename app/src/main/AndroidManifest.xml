<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.ira_1202150251_modul2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".menuutama" />
        <activity android:name=".menu" />
        <activity android:name=".dinein" />
        <activity android:name=".takeaway" />
        <activity android:name=".detail" android:parentActivityName=".menu">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".detail"/>
        </activity>
        // command tambahan diatas adalah untuk menambahkan tombol back pada detail menu
    </application>

</manifest>